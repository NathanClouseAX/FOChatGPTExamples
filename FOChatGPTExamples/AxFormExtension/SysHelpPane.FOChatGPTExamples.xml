				<Name>ChatGPTAssistant</Name>
				<Type>TabPage</Type>
				<FormControlExtension
					i:nil="true" />
				<Controls>
					<AxFormControl xmlns=""
						i:type="AxFormStaticTextControl">
						<Name>AssistancePromptText</Name>
						<Type>StaticText</Type>
						<FormControlExtension
							i:nil="true" />
